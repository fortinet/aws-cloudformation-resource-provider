{
    "typeName": "Fortinet::FortiGate::SystemAdmin",
    "description": "Add an Admin User to a Fortigate.",
    "sourceUrl": "https://github.com/fortinet/cloudformations-handlers",
    "definitions": {},
    "properties": {
        "FortigateIP": {
            "type": "string",
            "description": "FortiGate Ip Or hostname, Do not include https://",
            "pattern": "[a-zA-Z0-9_.-]+"

        },
        "APIKey": {
            "type": "string",
            "description": "Token Generated by Fortigate",
            "pattern": "[a-zA-Z0-9_.-]+"
        },
        "Accprofile": {
            "type": "string",
            "description": "Access profile for this administrator. Access profiles control administrator access to FortiGate features.",
            "pattern": "[a-zA-Z0-9_.-]+"
        },
        "Password": {
            "type": "string",
            "pattern": "[a-zA-Z0-9_.-]+",
            "description": "Admin user password."
        },
        "Name":{
            "type":"string",
            "pattern": "[a-zA-Z0-9_.-]+"
        }
    },
    "required": [
        "APIKey",
        "FortigateIP",
        "Accprofile",
        "Password"

    ],
    "primaryIdentifier": [
        "/properties/Name"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "SystemAdmin:Create"
            ]
        },
        "update": {
            "permissions": [
                "SystemAdmin:Update"
            ]
        },
        "delete": {
            "permissions": [
                "SystemAdmin:Delete"
            ]
        }


    }
}